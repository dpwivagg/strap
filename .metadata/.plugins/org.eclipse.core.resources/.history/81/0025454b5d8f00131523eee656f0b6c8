#ifndef CONSTANTS
#define CONSTANTS

// Drive system motor ports
static const UINT32 LEFT_MOTOR_PORT = 1;
static const UINT32 RIGHT_MOTOR_PORT = 2;

// Driver joystick ports
static const UINT32 JOYSTICK_PORT = 1;

// Arm motor ports
static const UINT32 ARM_MOTOR_PORT = 3;

// Pneumatic system ports
static const UINT32 SPIKE_RELAY_PORT = 2;
static const UINT32 PRESSURE_SWITCH_PORT = 4;

#endif /* CONSTANTS */
